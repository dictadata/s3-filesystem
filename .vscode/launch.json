  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "AWS check",
      "program": "${workspaceFolder}/test/s3/aws_credentials.js"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "s3 list",
      "program": "${workspaceFolder}/test/s3/list.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "s3 transfers",
      "program": "${workspaceFolder}/test/s3/transfers.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "s3 copy",
      "program": "${workspaceFolder}/test/s3/copy.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    }
  ]
}
