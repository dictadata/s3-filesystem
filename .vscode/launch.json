  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Test",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "newfs list",
      "program": "${workspaceFolder}/test/newfs/list.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "debug"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "newfs transfer",
      "program": "${workspaceFolder}/test/newfs/transfer.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    }
  ]
}
